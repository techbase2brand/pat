      body {
        font-family: 'Montserrat', sans-serif;
        font-style: normal;
        font-weight: 400;
        font-size: 13px;
        line-height: 20px;
        letter-spacing: -0.32px;
        color: #78696a;
        margin: 0;
        background-color: #fffbf7;
      }

      h1 {
        font-family: 'Canela', serif;
        font-style: normal;
        font-weight: 100;
        font-size: 50px;
        line-height: 56px;
        color: #3e2b2e;
        letter-spacing: unset;
        margin: 0;
      }

      h3 {
        font-family: 'Canela', serif;
        font-style: normal;
        font-weight: 300;
        font-size: 28.8272px;
        line-height: 38px;
        color: #3e2b2e;
        letter-spacing: unset;
        margin: 0;
      }

      .container {
        width: 100%;
        max-width: 600px;
        margin: auto;
      }

      .bg-light-tan {
        background-color: #f3e9e2;
      }

      .w-full {
        width: 100%;
      }

      .text-center {
        text-align: center;
      }

      .img-wrapper {
        padding: 120px 50px;
      }

      .img-gift-card {
        width: 100%;
        height: auto;
      }

      .content-title {
        margin: 58px 31px 0;
      }

      .content-body {
        margin: 20px auto 0;
        max-width: 244px;
      }

      .button {
        cursor: pointer;
        display: inline-flex;
        justify-content: center;
        text-decoration: none;
        align-items: center;
        font-family: 'Montserrat', sans-serif;
        font-size: 13px;
        line-height: 16px;
        letter-spacing: -0.32px;
        font-weight: 600;
        border: double 1px transparent;
        box-sizing: border-box;
        color: #fffbf7;
        background-color: #3e2b2e;
        transition: all 100ms ease-out;
      }

      .button:hover,
      .button:focus {
        background-origin: border-box;
        background-clip: padding-box, border-box;
        box-shadow: 4px 4px 12px #d2b29a66, -4px -4px 12px #fffbf7;
        background-image: linear-gradient(#e4cebe, #e4cebe),
          linear-gradient(90deg, #d3cba1 0%, #777467 25.92%, #dcd9ca 58.98%, #ada791 100%);
        color: #3e2b2e;
      }

      .button.fluid {
        width: 100%;
      }

      .actions__cell {
        padding-top: 40px;
      }

      .button__large {
        max-width: 552px;
        margin: auto;
        height: 51px;
      }

      .footer__logo {
        padding-top: 99px;
        padding-bottom: 1.49px;
      }

      .footer__logo-wrapper {
        position: relative;
      }

      .footer__logo-wrapper::before {
        content: ' ';
        position: absolute;
        bottom: 0px;
        left: 0;
        height: 70%;
        width: 100%;
        background: linear-gradient(180deg, rgba(250, 244, 239, 0) 0%, #faf4ef 100%);
      }

      .footer__logo-wrapper img {
        width: 100%;
        height: auto;
      }

      .footer {
        background-color: #faf4ef;
        padding-bottom: 83.13px;
      }

      .footer * {
        color: #3e2b2e;
      }

      .footer__cell a {
        color: #3e2b2e;
        text-decoration: none;
        cursor: pointer;
      }

      .footer__cell-links {
        padding-top: 8px;
        padding-left: 39.04px;
      }

      .footer__cell-links a {
        font-family: 'Montserrat', sans-serif;
        font-style: normal;
