  <body>
    <table class='body w-full' cellspacing='0' cellpadding='0'>
      <tr>
        <td>
          <table class='row content w-full bg-light-tan' cellspacing='0' cellpadding='0'>
            <tr>
              <td class='content__cell'>
                <center>
                  <table class='container' cellspacing='0' cellpadding='0'>
                    <tr>
                      <td class='img-wrapper'>
                        <img
                          loading='lazy'
                          class='img-gift-card'
                          src='https://cdn.shopify.com/s/files/1/0615/5265/6623/files/gift-card.png?v=1670520420'
                          alt='Gift card image'
                          height='400'
                          width='582'
                        >
                      </td>
                    </tr>
                  </table>
                </center>
              </td>
            </tr>
          </table>
          <table class='row content w-full' cellspacing='0' cellpadding='0'>
            <tr>
              <td class='content__cell'>
                <center>
                  <table class='container text-center' cellspacing='0' cellpadding='0'>
                    <tr>
                      <td>
                        <h1 class='content-title'>{{ email_title }}</h1>
                        <p class='content-body'>{{ email_body }}</p>

                        <table class='row actions w-full' cellspacing='0' cellpadding='0'>
                          <tr>
                            <td class='actions__cell'>
                              <table class='main-action-cell w-full'>
                                <tr>
                                  <td class='button__cell'>
                                    <a href='{{ gift_card.url }}' class='button fluid button__large'>View Gift Card</a>
                                  </td>
                                </tr>
                              </table>
                            </td>
                          </tr>
                        </table>

